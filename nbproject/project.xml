<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.cnd.makeproject</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/make-project/1">
            <name>anna_elsa_esp32_firmware</name>
            <c-extensions>c</c-extensions>
            <cpp-extensions>cc,cpp</cpp-extensions>
            <header-extensions>cstub,def,h,hpp,inc</header-extensions>
            <sourceEncoding>UTF-8</sourceEncoding>
            <make-dep-projects/>
            <sourceRootList>
                <sourceRootElem>.</sourceRootElem>
                <sourceRootElem>../esp/esp-idf-lib/components</sourceRootElem>
                <sourceRootElem>../esp/esp-idf/components</sourceRootElem>
                <sourceRootElem>../esp/esp-adf/components</sourceRootElem>
            </sourceRootList>
            <confList>
                <confElem>
                    <name>Default</name>
                    <type>0</type>
                </confElem>
            </confList>
            <formatting>
                <project-formatting-style>false</project-formatting-style>
            </formatting>
        </data>
        <preferences xmlns="http://www.netbeans.org/ns/auxiliary-configuration-preferences/1">
            <module name="org-netbeans-modules-html-editor-lib">
                <property name="default-html-public-id" value="HTML5"/>
            </module>
            <module name="org-netbeans-modules-javascript2-editor"/>
            <module name="org-netbeans-modules-editor-indent">
                <node name="CodeStyle"/>
            </module>
        </preferences>
        <spellchecker-wordlist xmlns="http://www.netbeans.org/ns/spellchecker-wordlist/1">
            <word>httpcontroller</word>
            <word>initialise</word>
        </spellchecker-wordlist>
    </configuration>
</project>
